{
  "include": ["**/*.py"],
  "exclude": ["**/node_modules",
              "**/__pycache__",
              "src/experimental",
              "src/typestubs"
  ],
  "ignore": ["src/oldstuff"],
  "defineConstant": { "DEBUG": true },
  "venvPath": ".",
